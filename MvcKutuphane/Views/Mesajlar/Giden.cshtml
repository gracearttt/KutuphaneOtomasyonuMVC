@using MvcKutuphane.Models.Entity
@model List<tblmesajlar>
@{
    ViewBag.Title = "Giden";
    Layout = "~/Views/Shared/_LayoutPanel.cshtml";
}


<h2>Giden Mesajlar Sayfası</h2>
<br />
<table class="table table-bordered">
    <tr>
        <td>Alıcı</td>
        <td>Konu</td>
        <td>Mesaj</td>
        <td>Tarih</td>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.alici</td>
            <td>@x.konu</td>
            <td>@x.icerik</td>
            <td>@x.tarih</td>

        </tr>
    }
</table>
